    "autoprefixer": "^10.4.2",
    "daisyui": "^2.0.0-next.2",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.6",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "tailwindcss": "^3.0.22",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
